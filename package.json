{
  "name": "phoenix",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "gateway",
    "worker",
    "crawl-service",
    "streamlit"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "dev:gateway": "turbo run dev --filter=gateway",
    "dev:worker": "turbo run dev --filter=worker",
    "dev:crawl": "turbo run dev --filter=crawl-service",
    "dev:streamlit": "turbo run dev --filter=streamlit"
  },
  "devDependencies": {
    "turbo": "^2.0.0"
  },
  "packageManager": "npm@10.0.0"
}
